//using fixed sliding window

int birthday(vector<int> s, int d, int m) {
    int n=s.size();
    int sum=0,count=0;
    for (int i=0 ; i<m ; i++) {
        sum+=s[i];
    }
    if (sum==d) count++;
    
    for (int i=m ; i<n ; i ++) {
        sum+=s[i];
        sum-=s[i-m];
        if (sum==d) count++;
    }
    return count;
}
