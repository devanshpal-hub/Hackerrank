int jumpingOnClouds(vector<int> c) {
    int n= c.size();
    int count=0;
    for (int i=0 ; i<n ;) {
        if(c[i+2]==0 && i+2<n) {
            count++;
            i+=2;
        }
        else if(c[i+1]==0 && i+1<n){
            count++;
            i+=1;
        }
        else i++;
    }
    return count;
}
