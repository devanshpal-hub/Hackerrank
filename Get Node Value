

int getNode(SinglyLinkedListNode* llist, int positionFromTail) {
     int n = positionFromTail;
     SinglyLinkedListNode* temp=llist;
     int a=0;
     while(temp!=nullptr) {
        temp=temp->next;
        a++;
     }
     int b=a-n;
     temp=llist;
     for (int i=0 ; i<b-1 ; i++) {
        temp=temp->next;
     }
     return temp->data;
}
