

vector<int> cutTheSticks(vector<int> arr) {
    vector<int> result;
    sort(arr.begin(),arr.end());
    int n=arr.size();
    
    int i=0;
    while(i<n) {
        result.push_back(n-i);
        int cut=arr[i];
        
        while(i<n && arr[i]==cut) {
            i++;
        }
    }
    return result;
}
