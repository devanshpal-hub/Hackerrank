//using recursion

void reversePrint(SinglyLinkedListNode* llist) {
   
    if (llist==nullptr) return;
    reversePrint(llist->next);    
    cout<<llist->data<<endl;

}

//using stack

void reversePrint(SinglyLinkedListNode* llist) {
   
   stack<int> st;
   SinglyLinkedListNode *temp=llist;
   while(temp!=nullptr) {
    st.push(temp->data);
    temp=temp->next;
   }
   while(!st.empty()) {
    int i=st.top();
    cout<<i<<endl;
    st.pop();
   }
}
